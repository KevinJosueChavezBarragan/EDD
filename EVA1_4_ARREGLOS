//TDA --> Tipos Abstractos de Datos
package eva1.pkg4.arreglos;

public class EVA1_4_ARREGLOS {

    public static void main(String[] args) {
        //ARREGLO DE ENTEROS PARA CAPTURAR EDADES
        //TODOS LOS IDENTIFICADORES DECLARADOS SON DE TIPO ENTERO
        int[] arrayEdades, arraySalario, arrayCali;
        //SOLO ARREGGLOPROMEDIO ES ARREGLO
        double arrayPromedio[], porcentajes;
        //LOS ARREGLOS SON OBJETOS
        arrayEdades = new int[50];
        System.out.println(arrayEdades);
        for (int i = 0; i < arrayEdades.length; i++) {
            arrayEdades[i] = (int) (Math.random() * 100);
        }
        for (int i = 0; i < arrayEdades.length; i++) {
            System.out.println("["+ i + "] = " + arrayEdades[i]);
        }
    }
}
